package br.edu.fatecfranca.ex0;

public class Testa {

	public static void main(String[] args) {
		System.out.println("Hoje é 18_02_2022");
		
		
		// criar um objeto 
		Carro obj1 = new Carro();
		obj1.modelo = "Onix";
		obj1.ano = 2019;
		obj1.velocidade = 0;
		obj1.motor = false;
		
		obj1.ligar();
		obj1.mostrar();
		
		obj1.acelerar(80);
		obj1.mostrar();
		
		obj1.frear(20);
		obj1.mostrar();
		
		obj1.desligar();
		obj1.mostrar();
		
		Carro obj2 = new Carro("Palio", 2012, 0, false);
		
	}
}
